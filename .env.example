# Example .env file (DO NOT commit a real .env to git)

# Dhan API credentials (used by Server A feed service for live market access)
DHAN_CLIENT_ID=your_dhan_client_id_here
DHAN_ACCESS_TOKEN=your_dhan_access_token_here

# Redis password (if you run Redis with a password)
REDIS_PASSWORD=change_me

# Postgres passwords
POSTGRES_PASSWORD_MDS=change_me
POSTGRES_PASSWORD_TRADES=change_me

# MDS base URL (Server A adapter)
MDS_BASE_URL=http://localhost:8002/v1

# Other runtime configuration
MARKET_DATA_PROVIDER=mds
FEED_POLL_SECONDS=1

